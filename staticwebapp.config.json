{
	"routes": [
		{
			"route": "/Lessons",
			"rewrite": "/Pages/Lessons/Setup/Setup.html"
		}
	],
	"navigationFallback": {
		"rewrite": "index.html",
		"exclude": ["/images/*", "/css/*"]
	},
	"globalHeaders": {
		"content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
	},
	"mimeTypes": {
		".json": "text/json"
	}
}
